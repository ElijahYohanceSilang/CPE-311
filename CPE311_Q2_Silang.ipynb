    "def det(n):\n",
    "    i = 0\n",
    "    sum = 0\n",
    "    x = n\n",
    "    \n",
    "    while i < len(n):\n",
    "        sum += x[i]\n",
    "        print(sum)\n",
    "        i += 1\n",
    "    else:\n",
    "        return 0\n",
    "\n",
    "n = [20, 40, 60, 80, 100]\n",
    "det(n)"
